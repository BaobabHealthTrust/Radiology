<html>
<head>
<%= javascript_include_tag "prototype" %>
<%= stylesheet_link_tag "cohort" %>
<script type="text/javascript" language="javascript"
    src="/touchscreentoolkit/lib/javascripts/touchScreenToolkit.js" defer  ></script>
<style type="text/css">

     body {
            font-family: Arial,sans-serif;
          }

     .bold {
            font-weight: bold;
            font-size: 1em;
           }
      .cell {
            width: 25%;
            }
      .blank{
            height: 50px;color:#ffffff;
            }
       #head{
            padding-left: 40px;
            }

       table {
            border-style:solid;
            border-width:1px;
           
             }
        th {
            border-style:solid;
            border-width:1px;
           }
        td {
            text-align:center;
            border-style:solid;
            border-width:1px;
            font-size:2em;
         }
  </style>

<script>
  var tt_cancel_destination = "/clinic"
</script>
</head>
 <div id="reportData" style="position:relative; width:100%; height:85%; overflow:auto;">
    <%= stylesheet_link_tag "report" %>
<%if @report_type == "INVESTIGATIONS_DAILY" %>
 <%= render :partial => 'investigations_daily' %>
<%elsif @report_type == "INVESTIGATIONS" %>
 <%= render :partial => 'investigations' %>
<%elsif @report_type == "FILM SIZE" %>
 <%= render :partial => 'film_size' %>
<% end %>
 </div>
</html>
